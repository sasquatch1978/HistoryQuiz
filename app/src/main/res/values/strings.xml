    <!-- Good luck toast. It will be shown in the format of "Good luck, Bob." where Bob is the user's name. [CHAR LIMIT=NONE] -->
    <string name="luck">Good luck, <xliff:g example="Bob" id="person">%s</xliff:g>.</string>
